(* Archivo 3 *)

let delim = [' ''\t''\n']
let ws = delim+
let digit = ["0123456789"]
let digits = digit+
let number = digits(.digits)?('E'['+''-']?digits)?

rule tokens = 
    ws        { return WHITESPACE }               
  | number    { return NUMBER }
  | '+'       { return PLUS }
  | '*'       { return TIMES }
  | '('       { return LPAREN }
  | ')'       { return RPAREN }